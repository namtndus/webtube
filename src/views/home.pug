extends base.pug

block content
    h2 Welcome here you will see the trending video
    input(placeholder="whats ypur name")
    each video in videos 
        div 
            h4=video.title
            ul 
                li #{video.rating}/5.
                li #{video.comments} comments .
                li Posted #{video.createdAt}.
                li #{video.views} views.
    else
        li Sorry nothing found.  